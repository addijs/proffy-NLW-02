{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts --ignore ./src/__tests__",
		"typeorm:env": "ts-node ./node_modules/typeorm/cli.js",
		"typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js",
		"entity:create": "yarn typeorm:env entity:create -c development -n",
    "migration:create": "yarn typeorm:env migration:create -c development -n",
    "migration:run": "yarn typeorm:env migration:run -c development",
    "seed:run": "yarn typeorm:seed seed --configName ormconfig.ts --connection development",
    "test": "jest --runInBand --silent",
    "test:log": "jest --runInBand"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "jest": "^26.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1"
  }
}
